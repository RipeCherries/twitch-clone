{
  "arrowParens": "always",
  "bracketSpacing": true,
  "endOfLine": "crlf",
  "insertPragma": false,
  "printWidth": 120,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "requirePragma": false,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "none",
  "useTabs": false,
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": true,
  "importOrderParserPlugins": [
    "classProperties",
    "decorators-legacy",
    "typescript"
  ],
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@(.*)$",
    "^../(.*)",
    "^./(.*)"
  ],
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}